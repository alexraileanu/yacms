<!doctype html>
<html lang='en'>
<head>
    <meta charset='UTF-8'>
    <meta name='viewport'
          content='width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0'>
    <meta http-equiv='X-UA-Compatible' content='ie=edge'>

    <!-- UIkit CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-beta.30/css/uikit.min.css"/>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,700" rel="stylesheet">
    <link rel="stylesheet" href="{{ url_for('backend.static', filename='css/style.css') }}">

    <!-- Include external CSS. -->
    <link href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet"
          type="text/css"/>

    <!-- Include Editor style. -->
    <link href="//cdnjs.cloudflare.com/ajax/libs/froala-editor/2.7.0/css/froala_editor.pkgd.min.css" rel="stylesheet"
          type="text/css"/>
    <link href="//cdnjs.cloudflare.com/ajax/libs/froala-editor/2.7.0/css/froala_style.min.css" rel="stylesheet"
          type="text/css"/>

    <title>YaCMS</title>
</head>
<body>
<div class='uk-section-primary uk-preserve-color'>
{#    <div uk-sticky='animation: uk-animation-slide-top; sel-target: .uk-navbar-container; cls-active: uk-navbar-sticky; cls-inactive: uk-navbar-transparent uk-light; top: 200'>#}
    <div uk-sticky='media: 960' class='uk-navbar-container tm-navbar-container uk-sticky uk-sticky-fixed uk-active uk-sticky-below' style='position: fixed; top: 0px; width: 1416px;'>
        <nav class='uk-navbar-container'>
            <div class='uk-container uk-container-expand'>
                <nav uk-navbar>
                    {% block menu %}

                    {% endblock %}
                </nav>
            </div>
        </nav>
    </div>
</div>

{% block sidebar %}

{% endblock %}

{% block content %}

{% endblock %}

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-beta.30/js/uikit.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-beta.30/js/uikit-icons.min.js"></script>

<script type="text/javascript"
        src="//cdnjs.cloudflare.com/ajax/libs/froala-editor/2.7.0/js/froala_editor.pkgd.min.js"></script>

<script src='{{ url_for('backend.static', filename='js/app.js') }}'></script>
</body>
</html>